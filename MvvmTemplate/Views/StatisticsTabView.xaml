<UserControl
    x:Class="MvvmTemplate.Views.StatisticsTabView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:MvvmTemplate.ViewModels"
    xmlns:v="clr-namespace:MvvmTemplate.Views"
    xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF">
    <Grid Name="TabContent">
        <ContentControl Content="{Binding Plot, Mode=OneTime}" />
    </Grid>
</UserControl>